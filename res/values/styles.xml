<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
   
    
    <style name="table_blank">
        <item name="android:layout_height">25dip</item>
        <item name="android:layout_width">25dip</item>
        <item name="android:background">#555555</item>        
        <item name="android:layout_margin">1.0px</item>
    </style>
    <style name="table_blank_small">
        <item name="android:layout_height">25dip</item>
        <item name="android:layout_width">10dip</item>
        <item name="android:background">#555555</item>
        <item name="android:layout_margin">1.0px</item>
    </style>
    
     <style name="table_item">
        <item name="android:layout_height">25dip</item>
        <item name="android:layout_width">25dip</item>
        <item name="android:textSize">10.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/a</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">1.0px</item>    
    </style>
    
</resources>

